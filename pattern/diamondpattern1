//AVI KASLIWAL
#include <stdio.h>

void main(){
	int sp,row,col,num;
	int count=0;
	for(row=1;row<=5;row++)
	{
		num=1;
		for(sp=1;sp<=5-row;sp++)
		{
			printf(" ");
		}
		for(col=1;col<=row+count;col++)
		{
				//num=1;
			printf("%d",num);
			num++;	
		}
	count++;	
	printf("\n");
	}
	count=4;
	for(row=1;row<=4;row++)
  	{
		num=1;
		for(sp=1;sp<=row;sp++)
		{
			printf(" ");
		}
		for(col=1;col<=4-row+count;col++)
    		{
			printf("%d",num);
			num++;
		}
	count--;	
	printf("\n");
	}
}
