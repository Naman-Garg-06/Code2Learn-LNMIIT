

#include <iostream>
#include <cmath>
using namespace std;
int main(){
	long long int n,t;
	cin>>n>>t;
	if(n==1 && t==10){
		cout<<"-1";
	}
	else if(n==1 && t<10){
		cout<<t;
	}
	else if(n>1 && t<10){
		long long int i;
		for(i=0;i<n;i++){
			cout<<t;
		}
	}
	else if(n>1 && t==10){
		long long int i;
		cout<<"10";
		for(i=0;i<n-2;i++){
			cout<<"0";
		}
	}
	return 0;
}
